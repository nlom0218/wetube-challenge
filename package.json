{
  "name": "wetube-challenge",
  "version": "1.0.0",
  "description": "wetube-challenge",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node init.js",
    "dev:assets": "cd src && webpack -w",
    "build:server": "babel src --out-dir build --ignore 'src/assets', 'src/static', 'src/webpack.config.js",
    "build:assets": "cd src && webpack",
    "copyAll": "xcopy src\\static build\\static /d /e /h /c /y /i && xcopy src\\views build\\views /d /e /h /c /y /i",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rd /s \"build\"",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js"
  },
  "author": "KHD",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "aws-sdk": "^2.849.0",
    "axios": "^0.21.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "mini-css-extract-plugin": "^1.3.6",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.5",
    "postcss-loader": "^5.0.0",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
